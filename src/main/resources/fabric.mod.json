{
  "schemaVersion": 1,
  "id": "parchment",
  "version": "${version}",

  "name": "Parchment",
  "icon": "assets/parchment/icon.png",
  "description": "Foundational lib for loading and executing scripts in Fabric",
  "license": "MIT",
  "contact": {
    "sources": "https://github.com/CottonMC/Parchment"
  },
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.cottonmc.parchment.Parchment"
    ],
    "parchment:engine_initializer": [
      "io.github.cottonmc.parchment.impl.NashornScriptInitializer"
    ]
  },
  "depends": {
    "fabricloader": ">=0.4.0"
  },
  "custom": {
    "badges": [ "library" ]
  }
}
